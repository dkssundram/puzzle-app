<!DOCTYPE html>
<html>
<head>
	<title>Admin Dashboard</title>
	<link href="/css/style5.css" rel="stylesheet" type="text/css">
</head>
<body>
	<center><h1>Admin Dashboard</h1></center>
	<form action="/logout" method="POST">
		<button type="submit">Logout</button>
	  </form>
	<table>
		<thead>
			<tr>
				<th>Id</th>
				<th>Username</th>
				<th>Email</th>
				<th>Password</th>
				<th>Score</th>
				<th>Incorrect</th>
				<th>Accuracy</th>
				
			</tr>
		</thead>
		<tbody>
			<% accounts.forEach(function(user) { %>
				<tr>
					<td><%= user.id %></td>
					<td><%= user.username %></td>
					<td><%= user.email %></td>
					<td><%= user.password %></td>
					<td><%= user.score %></td>
					<td><%= user.incorrect %></td>
					<td><%= user.accuracy %></td>
				</tr>
			<% }); %>
		</tbody>
	</table>
	
</body>
</html>
